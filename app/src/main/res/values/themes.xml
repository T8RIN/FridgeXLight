<resources>

    <style name="FridgeXLight" parent="Theme.MaterialComponents.DayNight.NoActionBar">

        <item name="colorPrimary">#E8E8E8</item>
        <item name="colorPrimaryVariant">#CCCCCC</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="bottomSheetDialogTheme">@style/ThemeOverlay.App.BottomSheetDialog</item>
        <item name="android:windowContentTransitions">true</item>

        <item name="windowActionModeOverlay">true</item>

        <item name="android:dialogTheme">@style/DialogTheme</item>

        <item name="colorSecondary">@color/checked</item>
        <item name="colorSecondaryVariant">#874B01</item>
        <item name="colorOnSecondary">@color/black</item>

        <item name="materialAlertDialogTheme">@style/modeAlert</item>
        <item name="fontFamily">@font/comfortaa_medium</item>
        <item name="android:textColor">@color/manualText</item>
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>

        <item name="android:colorEdgeEffect">@color/checked</item>
        <item name="android:textCursorDrawable">@drawable/cursor_color</item>

        <item name="colorError">@color/red</item>
        <item name="colorControlActivated">@color/checked</item>
        <item name="rippleColor">@color/checkedTrans</item>

        <item name="checked">@color/checked</item>
        <item name="checkedl">@color/checkedl</item>
        <item name="dchecked">#BA3702</item>
        <item name="checkedTrans">@color/checkedTrans</item>

        <item name="textFieldColor">?attr/dchecked</item>
        <item name="textErrorColor">@color/red</item>
    </style>

    <style name="FridgeXLight.Launcher">
        <item name="android:windowBackground">@drawable/splash</item>
    </style>

